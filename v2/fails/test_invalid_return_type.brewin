(class person
  (method void something () (print "hi"))
)

(class student inherits person
  (method voided something_else () (print "yo"))
  (method student foo ((student ref1) (person ref2)) (return me))
)

(class main
  (field student x1 null)
  (field person x2 null)
  (method student foo ((student ref1) (person ref2)) 
    (return ref1)
  )
  (method main main () 
    (begin
      (set x1 (new student))
      (set x2 (new person))
      (return me)
    )
  )
)